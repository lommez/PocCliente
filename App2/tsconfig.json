{
  "compilerOptions": {
    "module": "commonjs",
    "noImplicitAny": false,
    "noEmitOnError": true,
    "removeComments": false,
    "sourceMap": true,
    "target": "es6",
    "emitDecoratorMetadata": true,
    "experimentalDecorators": true,
    "outDir": "wwwroot/typescript/generated"
  },
  "include": [
      "wwwroot/**/*"
  ],
  "exclude": [
    "node_modules"
  ]
}