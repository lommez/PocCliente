{
  "compilerOptions": {
    "module": "commonjs",
    "noImplicitAny": false,
    "noEmitOnError": true,
    "removeComments": false,
    "sourceMap": true,
    "target": "es5",
    "baseUrl": ".",
    "outDir": "wwwroot/typescript/generated",
    "emitDecoratorMetadata": true,
    "experimentalDecorators": true
  },
  "include": [
      "wwwroot/**/*"
  ],
  "exclude": [
    "node_modules"
  ]
}